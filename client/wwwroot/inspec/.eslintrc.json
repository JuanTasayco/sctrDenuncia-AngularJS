{
  "env": {
    "browser": true,
    "node": true,
    "amd": true
  },
  "extends": [
    "eslint:recommended",
    "angular",
    "prettier"
  ],
  "plugins": [
    "prettier"
  ],
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 5
  },
  "globals": {
    "jinqJs": true
  },
  "rules": {
    "prettier/prettier": ["error", {
      "singleQuote": true,
      "bracketSpacing": false,
      "printWidth": 120
    }]
  }
}
