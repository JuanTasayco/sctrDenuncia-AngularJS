{
  "name": "referencia",
  "version": "1.0.0-rc.2",
  "description": "Sistema de Referencia y Contrareferencia",
  "main": "main.js",
  "sasslintConfig": "./.sass-lint.yml",
  "scripts": {
    "215": "gulp build --env 2158 --oim",
    "216": "gulp build --env 216 --oim",
	"prod": "gulp build --env prod --oim",
    "pre": "gulp build --env pre --oim",
	"int": "gulp build --env int --oim",
	"dev": "gulp build --env dev",
    "start": "concurrently --raw --kill-others  \"cross-env NODE_TLS_REJECT_UNAUTHORIZED=0 PORT=12000 nodemon --watch referenciaAPI referenciaAPI/app.js\"  \"sleep 2 & gulp default --env dev\"",
    "startw": "concurrently --raw --kill-others  \"cross-env NODE_TLS_REJECT_UNAUTHORIZED=0 PORT=12000 nodemon --watch referenciaAPI referenciaAPI/app.js\"  \"sleep 2 & gulp default --env dev\"",
    "start$": "concurrently --raw --kill-others  \"cross-env node referenciaAPI/app.js\"  \"gulp default --env dev\"",
    "serie": "nodemon --watch referenciaAPI referenciaAPI/app.js & gulp default --env dev",
    "inspect": "concurrently --raw --kill-others  \"cross-env NODE_TLS_REJECT_UNAUTHORIZED=0 PORT=12000 nodemon --debug --watch referenciaAPI referenciaAPI/app.js\" \"node-inspector --web-port 7070\" \"sleep 3 & gulp inspect --env dev --b chrome\" ",
    "http2": "concurrently --raw --kill-others  \"cross-env NODE_TLS_REJECT_UNAUTHORIZED=0 PORT=12000 nodemon --watch referenciaAPI referenciaAPI/app.js\"  \"sleep 2 & gulp http2 --env dev\"",
    "patch": "npm version patch",
    "minor": "npm version minor",
    "major": "npm version major",
    "test": "gulp bower && gulp test",
    "httpProxy": "gulp httpProxy",
    "buildenv": "gulp build",
    "lint": "gulp lint",
    "mock": "cross-env PORT=12000 nodemon --watch referenciaAPI referenciaAPI/app.js",
    "serve": "concurrently --raw --kill-others  \"cross-env NODE_TLS_REJECT_UNAUTHORIZED=0 PORT=12000 nodemon --watch referenciaAPI referenciaAPI/app.js\"  \"sleep 2 & gulp httpProd --env prod\""
  },
