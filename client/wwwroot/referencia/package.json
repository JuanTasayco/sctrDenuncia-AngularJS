  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    },
    "ghooks": {
      "pre-commit": "gulp lint",
      "commit-msg": "validate-commit-msg",
      "pre-push": "gulp lint"
    },
    "validate-commit-msg": {
      "helpMessage": "\nPlease fix your commit message (and consider using http://npm.im/commitizen)\n",
      "types": [
        "feat",
        "fix",
        "docs",
        "style",
        "refactor",
        "perf",
        "test",
        "chore",
        "revert",
        "custom"
      ],
      "warnOnFail": false
    }
  }
